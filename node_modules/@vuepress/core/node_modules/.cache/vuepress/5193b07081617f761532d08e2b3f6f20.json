(time_el) {\n      let date = new Date();\n      let h = date.getHours();\n      let m = date.getMinutes();\n      let s = date.getSeconds();\n      let day_night = \"AM\";\n\n      if (h > 12) {\n        h = h - 12;\n        day_night = \"PM\";\n      }\n\n      if (h < 10) {\n        h = \"0\" + h;\n      }\n      if (m < 10) {\n        m = \"0\" + m;\n      }\n      if (s < 10) {\n        s = \"0\" + s;\n      }\n\n      time_el.textContent = h + \":\" + m + \":\" + s + \" \" + day_night;\n\n    }\n  }\n}\n"]}]}